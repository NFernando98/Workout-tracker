{
    "version": 2,
    "rewrites": [
        {"source": "/(.*)", "destination": "/"}
    ],
    ]
    "builds": [
      {
        "src": "backend/server.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/server.ts"
      }
    ]
  }
  
